{
    "name": "nuxt-app",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt dev --dotenv .env.dev",
        "dev-generate": "NODE_OPTIONS=--max-old-space-size=4096 nuxt generate --dotenv .env.dev",
        "stage-generate": "nuxt generate --dotenv .env.stage",
        "prod-generate": "NODE_OPTIONS=--max-old-space-size=4096 nuxt generate --dotenv .env.prod",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
        "lint-fix": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix .",
        "prepare": "husky"
    },
    "dependencies": {
        "@floating-ui/vue": "^1.0.6",
        "@pinia/nuxt": "^0.5.1",
        "@vueuse/nuxt": "^10.9.0",
        "axios": "^1.6.7",
        "chart.js": "^4.4.6",
        "chartjs-plugin-datalabels": "^2.2.0",
        "dayjs": "^1.11.10",
        "floating-vue": "^5.2.2",
        "nuxt": "^3.10.1",
        "pinia": "^2.1.7",
        "vue": "^3.4.15",
        "vue-chart-3": "^3.1.8",
        "vue-router": "^4.2.5"
    },
    "devDependencies": {
        "@nuxtjs/eslint-config-typescript": "^12.1.0",
        "autoprefixer": "^10.4.17",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "husky": "^9.0.11",
        "postcss": "^8.4.34",
        "prettier": "^3.2.5",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.3.3",
        "vue-tsc": "^1.8.27"
    }
}
